{
  "name": "clean-node-api",
  "version": "1.1.0",
  "description": "Nodejs Rest API using TDD, Clean Architecture and Typescript",
  "scripts": {
    "start": "ts-node src/main/server.ts",
    "start:dev": "npx nodemon src/main/server.ts",
    "test": "npx jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "npx jest --passWithNoTests",
    "test:unit": "npm test -- --watch --config jest-unit-config.js",
    "test:integration": "npm test -- --watch --config jest-integration-config.js",
    "test:staged": "npm test -- --detectOpenHandles",
    "test:ci": "npm test -- --coverage",
    "prepare": "husky install"
  },
  "author": "Matheus Martins",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.12.1",
    "validator": "^13.7.0"
  }
}
